{
  "name": "@formspree/react",
  "version": "2.2.4",
  "description": "The React component library for Formspree",
  "bugs": {
    "url": "https://github.com/formspree/formspree-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/formspree/formspree-react.git"
  },
  "license": "MIT",
  "author": "Derrick Reimer",
  "files": [
    "dist/**/*.{js,ts}"
  ],
  "main": "dist/formspree-react.cjs.js",
  "module": "dist/formspree-react.esm.js",
  "types": "dist/types/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run build:types && npm run build:js",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "format": "npx prettier --write 'src/**/*.{js,ts}'",
    "prepare": "npm run build",
    "release": "np",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,json,md}": [
      "npx prettier --write",
      "git add"
    ],
    "package.json": [
      "npx sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@formspree/core": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.6.3",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "babel-jest": "^26.3.0",
    "husky": "^3.0.5",
    "jest": "^26.4.2",
    "lint-staged": "^9.2.5",
    "np": "^6.4.0",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sort-package-json": "^1.22.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "np": {
    "yarn": false
  }
}
