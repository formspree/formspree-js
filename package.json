{
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=@formspree/cra-demo^... && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "eslint": "^8.15.0",
    "prettier": "^2.5.1",
    "turbo": "^1.10.3"
  }
}
